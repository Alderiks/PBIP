table 'Calc group Time intelligence'
	lineageTag: 23fc66f7-8af6-4df4-91bd-4b7b5188b465

	calculationGroup

		calculationItem Waarde = SELECTEDMEASURE()

		calculationItem 'MTD maar niet voor avg verkopen' =
				
				IF(NOT ISSELECTEDMEASURE([avg verkopen]),
				    CALCULATE(SELECTEDMEASURE(),
				    DATESMTD('Datum tabel'[Date])))

		calculationItem YTD =
				
				CALCULATE(SELECTEDMEASURE(),
				DATESYTD('Datum tabel'[Date]))

		calculationItem LASTDATE =
				
				CALCULATE(SELECTEDMEASURE(),LASTDATE('Datum tabel'[Date]))

		calculationItem 'Moving average' =
				CALCULATE(
				    AVERAGEX('Datum tabel',SELECTEDMEASURE()),
				    DATESINPERIOD('Datum tabel'[Date],
				    LASTDATE('Datum tabel'[Date]),
				    -7,DAY))

		calculationItem 'Vorige maand' = CALCULATE([Omzet],DATEADD('Datum tabel'[Date],-1,MONTH))

			formatStringDefinition = IF(SELECTEDMEASURE()>10,"00.00â‚¬","00$")

