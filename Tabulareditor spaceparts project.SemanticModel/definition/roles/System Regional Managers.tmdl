role 'System Regional Managers'
	modelPermission: read

	tablePermission Regions =
			
			VAR _CurrentUser =
			    SELECTCOLUMNS (
			        FILTER (
			            'Employees',
			            'Employees'[Employee Email]
			                = USERPRINCIPALNAME ( )
			        ),
			        "@Name", 'Employees'[Employee Name]
			    )
			RETURN
			    'Regions'[System Regional Managers] IN _CurrentUser

	annotation PBI_Id = eaa78ebe59c245ccab75056fa76172e9

