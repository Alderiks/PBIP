role 'System Sales Directors'
	modelPermission: read

	tablePermission Regions =
			
			VAR _CurrentUser =
			    SELECTCOLUMNS (
			        FILTER (
			            'Employees',
			            'Employees'[Employee Email]
			                = USERPRINCIPALNAME ( )
			        ),
			        "@Name", 'Employees'[Employee Name]
			    )
			RETURN
			    'Regions'[System Sales Directors] IN _CurrentUser

	annotation PBI_Id = e1130bdac9114fbabf57e2875f59743b

