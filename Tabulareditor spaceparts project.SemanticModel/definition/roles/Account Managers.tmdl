role 'Account Managers'
	modelPermission: read

	tablePermission Customers =
			
			VAR _CurrentUser =
			    SELECTCOLUMNS (
			        FILTER (
			            'Employees',
			            'Employees'[Employee Email]
			                = USERPRINCIPALNAME ( )
			        ),
			        "@Name", 'Employees'[Employee Name]
			    )
			RETURN
			    'Customers'[Account Manager] IN _CurrentUser

	annotation PBI_Id = dbdfbff19eea4f0d84010925e3c7f23e

